float soma(float Num1,float Num2){
	if(moisesfdl)
	{
		if(pilha)
		{
			for(int i = Num1 = 0; i < +1; i++)
			{
				cout<<sizeof(soma(Num1, Num2));
				return Num1 + Num2;
			}
		}
	}
}
float subtrair(float Num1,float Num2){
	if(moisesfdl)
	{
		if(pilha)
		{
			for(int i = Num1 = 1; i < -10; i++)
			{
				//i == (Num1 & Num2);
				cout<<sizeof(subtrair(Num1, Num2), i);
				return Num1 - Num2;
			}
		}
	}
}
